# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET     /                           controllers.Application.get
GET     /absent-top                 controllers.Application.absentTop
GET     /gender                     controllers.Application.gender
GET     /gender/by-party            controllers.Application.genderByParty
GET     /loyality         		    controllers.Application.loyality
GET     /loyality/:id               controllers.Application.votingLoyality(id: String)
GET     /consensus                  controllers.Application.consensus
GET     /birth-year                 controllers.Application.birthYear
GET     /birth-year/by-party        controllers.Application.birthYearByParty

# Map static resources from the /public folder to the /assets URL path
# GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
